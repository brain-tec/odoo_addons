<odoo>
    <data>
        <record id="view_cas_parameter_form" model="ir.ui.view">
            <field name="name">cas.parameter.form</field>
            <field name="model">cas.parameter</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="active"/>
                                <field name="cas_server_url"/>
                                <field name="cas_service_url"/>
                                <field name="cas_version"/>
                                <field name="update_user_attributes"/>
                                <field name="create_user_if_non_exists"/>
                                <field name="attributes_mapping_ids"/>
                                <field name="matching_attribute"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="smile_auth_cas_view_cas_parameter_tree" model="ir.ui.view">
            <field name="name">cas.parameter.tree</field>
            <field name="model">cas.parameter</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="active"/>
                    <field name="cas_server_url"/>
                    <field name="cas_service_url"/>
                </tree>
            </field>
        </record>

        <record id="action_cas_parameter" model="ir.actions.act_window">
            <field name="name">CAS Parameter</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cas.parameter</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'active_test': False}</field>
        </record>
    </data>
</odoo>